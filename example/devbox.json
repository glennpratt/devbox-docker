{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.16.0/.schema/devbox.schema.json",
  "packages": [
    "github:NixOS/nixpkgs/nixos-25.11#curl",
    "github:NixOS/nixpkgs/nixos-25.11#yq-go"
  ],
  "shell": {
    "init_hook": [
